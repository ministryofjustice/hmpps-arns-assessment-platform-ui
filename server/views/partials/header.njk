                      <span class="arns-common-header__oasys-account-text">
                        <span class="arns-common-header__oasys-account-label">OASys Account</span>
                        <span data-qa="header-user-name">{{ user.displayName | initialiseName }}</span>
                      </span>
                    </span>
                  </span>
                </div>
              </div>
            {% else %}
              {# HMPPS Auth user — dropdown #}
              <div class="arns-common-header__navigation__item">
                <button class="arns-common-header__menu-toggle arns-common-header__user-menu-toggle" aria-controls="arns-common-header-user-menu" aria-expanded="false" type="button" hidden="hidden">
                  <span>
                    {{ userSvg | safe }}
                    <span>
                      <span class="arns-common-header__menu-toggle-label">Account</span>
                      <span data-qa="header-user-name">{{ user.displayName | initialiseName }}</span>
                    </span>
                  </span>
                </button>
                <ul id="arns-common-header-user-menu" class="govuk-list arns-common-header__user-menu" hidden="hidden">
                  <li>
                    <a class="arns-common-header__submenu-link" href="/account-details">Your account</a>
                  </li>
                  <li>
                    <a class="arns-common-header__submenu-link" data-qa="signOut" href="/sign-out">Sign out</a>
                  </li>
                </ul>

                <div class="arns-common-header__icon-link-wrapper arns-common-header__user-menu-link">
                  <a class="arns-common-header__link" href="/account-details">
                    <span>
                      <span class="govuk-visually-hidden">Manage your account or sign out</span>
                      <span data-qa="header-user-name-fallback">{{ user.displayName | initialiseName }}</span>
                    </span>
                  </a>
                </div>
              </div>
            {% endif %}

          </div>
        </div>
      </div>
    </nav>
    {%- endif -%}
